{
  "name": "partner-it/cloud-images",
  "description": "CloudImages plugin for CakePHP",
  "type": "cakephp-plugin",
  "require": {
    "php": ">=5.6"
  },
  "suggest": {
    "cakephp/cakephp": ">=2.2"
  },
  "require-dev": {
    "phpunit/phpunit": "*"
  },
  "autoload": {
    "psr-4": {
      "CloudImages\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CloudImages\\Test\\": "tests",
      "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
    }
  }
}
